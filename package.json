{
  "name": "min-to-win",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start:rest": "run-p watch:rest ** netlify dev",
    "watch:rest": "onchange ./server/src-rest/**/*.js -- npm run bundle:rest",
    "bundle:rest": "cpx server/src-rest/**/* functions/",
    "build": "run-p bundle:rest && build:client",
    "build:client": "cd client && npm run build"
  },
