  },
  "homepage": "https://github.com/geraldiner/min-to-win#readme",
  "devDependencies": {
    "cpx": "^1.5.0",
    "onchange": "^7.1.0"
  },
  "dependencies": {
    "npm-run-all": "^4.1.5"
  }
}